{
  "manifestVersion": 1,
  "id": "code-coverage-widget-dev",
  "name": "Code Coverage Widget Dev",
  "description": "Dashboard widget that shows pie charts of code coverage from the last build for selected pipelines.",
  "version": "1.0.13",
  "publisher": "JaderCM",
  "content": {
    "details": {
      "path": "DETAILS.md"
    },
    "license": {
      "path": "LICENSE"
    },
    "privacypolicy": {
      "path": "PRIVACY.md"
    }
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "categories": [
    "Azure Boards"
  ],
  "icons": {
    "default": "web/img/icon.png"
  },
  "files": [
    {
      "path": "web",
      "addressable": true
    }
  ],
  "links": {
    "privacypolicy": {
      "uri": "PRIVACY.md"
    },
    "repository": {
      "uri": "https://github.com/JaderCM/azure-code-coverage-widget"
    },
    "support": {
      "uri": "https://marketplace.visualstudio.com/items?itemName=JaderCM.code-coverage-widget-dev&ssr=false#qna"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/JaderCM/azure-code-coverage-widget"
  },
  "screenshots": [
    {
      "path": "web/img/screenshot_dark.png"
    }
  ],
  "branding": {
    "color": "#106ebe",
    "theme": "light"
  },
  "contributions": [
    {
      "id": "code-coverage-widget-dev",
      "type": "ms.vss-dashboards-web.widget",
      "targets": [
        "ms.vss-dashboards-web.widget-catalog",
        "JaderCM.code-coverage-widget-dev.code-coverage-widget-dev-configuration"
      ],
      "properties": {
        "name": "Code Coverage Widget Dev",
        "description": "Show code coverage as pies for selected pipelines.",
        "icon": "web/img/icon.png",
        "catalogIconUrl": "web/img/icon.png",
        "previewImageUrl": "web/img/icon.png",
        "supportedScopes": ["project_team"],
        "uri": "web/index.html",
        "isNameConfigurable": true,
        "supportedSizes": [
          { "rowSpan": 2, "columnSpan": 2 },
          { "rowSpan": 2, "columnSpan": 3 },
          { "rowSpan": 2, "columnSpan": 4 },
          { "rowSpan": 2, "columnSpan": 5 },
          { "rowSpan": 2, "columnSpan": 6 },
          { "rowSpan": 2, "columnSpan": 7 },
          { "rowSpan": 2, "columnSpan": 8 },
          { "rowSpan": 2, "columnSpan": 9 },
          { "rowSpan": 2, "columnSpan": 10 },
          { "rowSpan": 3, "columnSpan": 2 },
          { "rowSpan": 4, "columnSpan": 2 },
          { "rowSpan": 5, "columnSpan": 2 },
          { "rowSpan": 6, "columnSpan": 2 },
          { "rowSpan": 7, "columnSpan": 2 },
          { "rowSpan": 8, "columnSpan": 2 },
          { "rowSpan": 9, "columnSpan": 2 },
          { "rowSpan": 10, "columnSpan": 2 }
        ]
      }
    },
    {
      "id": "code-coverage-widget-dev-configuration",
      "type": "ms.vss-dashboards-web.widget-configuration",
      "targets": [ "ms.vss-dashboards-web.widget-configuration" ],
      "properties": {
        "name": "Code Coverage Widget Dev Configuration",
        "description": "Show code coverage as pies for selected pipelines.",
        "uri": "web/config.html"
      }
    }
  ],
  "scopes": [ "vso.build", "vso.test" ]
}
